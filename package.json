  "devDependencies": {
    "@types/jest": "^29.2.0",
    "jest": "^29.2.0",
    "prettier": "^2.7.1",
    "ts-jest": "^29.0.3",
    "tslint": "^6.1.3",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^4.8.4"
  },
  "dependencies": {
    "fp-ts": "^2.13.1",
    "io-ts": "^2.2.19"
